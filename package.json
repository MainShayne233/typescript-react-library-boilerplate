{
  "name": "typescript-react-library-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for starting a React library",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "NODE_ENV=production webpack -p",
    "test": "NODE_ENV=test mocha-webpack test/**/*.ts*",
    "test:watch": "yarn test -- --watch",
    "typecheck": "tsc",
    "typecheck:watch": "yarn typecheck -- --watch",
    "lint": "eslint {src,test}/**/*.js* webpack.config.js",
    "format": "prettier {src,test}/**/*.js  webpack.config.js  --write",
    "precommit:install": "./node_modules/precommit/bin/cli.js install -c .precommit.json",
    "precommit:uninstall": "./node_modules/precommit/bin/cli.js remove"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "babel-plugin-rewire"
        ]
      }
    }
  },
  "author": "MainShayne233",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jsdoc-test": "0.0.9",
    "mocha": "^5.0.5",
    "precommit": "^1.2.2",
    "prettier": "^1.11.1",
    "react-hot-loader": "^4.0.0",
    "webpack": "^4.2.0",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "@types/chai": "^4.1.4",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.7.1",
    "@types/prop-types": "^15.5.5",
    "@types/react-dom": "^16.0.7",
    "babel-plugin-rewire": "^1.1.0",
    "jsdom": "^13.2.0",
    "jsdom-global": "^3.0.2",
    "mocha-webpack": "^2.0.0-beta.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1",
    "webpack-cli": "3.1.1"
  }
}
